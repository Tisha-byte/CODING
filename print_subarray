#include <iostream>
using namespace std;
void print_subarray(int arr[],int n)
{
for(int i=0;i<n;i++)
{
    for(int j= i;j<=n;j++)
    {
        for(int k=i;k<j;k++)
        {
            cout<<arr[k]<<" ";
        }
        cout<<endl;
    }
}
    
}
using namespace std;

int main() {
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    cin>>a[i];
    print_subarray(a,n);
    
    

    return 0;
}
