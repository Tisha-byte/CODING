#include <iostream>
using namespace std;
void print_pairs(int arr[],int n)
{
    for(int i=0;i<n;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            cout<<"("<<arr[i]<<" ,"<<arr[j]<<")"<<endl;
        }
    }
}
using namespace std;

int main() {
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    cin>>a[i];
    print_pairs(a,n);

    return 0;
}
